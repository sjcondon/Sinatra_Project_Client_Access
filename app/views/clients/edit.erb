<h1>Edit Client for <%= @client.name %></h1>
<p><%= @error %></p>
<form action="/clients/<%=@client.id%>" method="post">
    <label>Name</label>
    <input type="text" name="name" value="<% @client.name %>">
    <label>Bio</label>
    <textarea name="bio" value="<% @client.bio %>"></textarea>
    <label>Image URL</label> 
    <input type="text" name="image" value="<% @client.image %>">
    <button type="submit">Edit Client</button>
</form>